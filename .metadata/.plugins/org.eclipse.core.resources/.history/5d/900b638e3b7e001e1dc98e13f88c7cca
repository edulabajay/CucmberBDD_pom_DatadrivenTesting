	static WebDriver driver;
	
	static Properties prop;
	
	
	
	public Basepage_pom() {
		
		try {
			
			prop = new Properties();
			FileInputStream ip=new FileInputStream("");
			prop.load(ip);	
	
		}catch (FileNotFoundException e) {
			
			
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	
	
	}
	
	
}
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	

